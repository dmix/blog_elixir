<h2>Edit Category</h2>

<%= if current_user(@conn) do %>
  <%= render "form.html", changeset: @changeset,
                          action: category_path(@conn, :update, @category) %>
<% end %>

<div class="lh-copy mt3">
    <%= link "Back", to: category_path(@conn, :index), class: "f6 link dim black db" %>
</div>
